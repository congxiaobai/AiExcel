{
  "extends": "@electron-toolkit/tsconfig/tsconfig.web.json",
  "include": [
    "src/renderer/src/env.d.ts",
    "src/renderer/src/**/*",
    "src/renderer/src/**/*.tsx",
    "src/preload/*.d.ts"
, "src/main/TongyiConnect.ts"  ],
  "compilerOptions": {
    "composite": true,
    "noImplicitAny": false,
    "strict": false,
    "allowJs": true,
    "esModuleInterop":true,
    "jsx": "react",
    
    "baseUrl": ".",
    "paths": {
      "@renderer/*": [
        "src/renderer/src/*"
      ]
    }
  }
}
